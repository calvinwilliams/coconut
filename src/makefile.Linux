# 提交安装规则
.PHONY:install
install		:	all

ifneq ($(strip $(BININST)),)
	@if [ ! -d "$(BININST)" ] ; then \
		echo $(MKDIR) $(BININST) ; \
		$(MKDIR) $(BININST) ; \
	fi
ifneq ($(strip $(BIN)),)
	@for FILE in `echo $(BIN)` ; do \
		if [ -f $(BININST)/$$FILE ] ; then \
			$(RM) $(BININST)/$$FILE ; \
		fi ; \
		if [ -f $$FILE ] ; then \
			echo $(CP) $$FILE $(BININST)/ ; \
			$(CP) $$FILE $(BININST)/ ; \
		fi ; \
	done
endif
endif
ifneq ($(strip $(BININST2)),)
